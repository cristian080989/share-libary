pipeline {
    agent any
    
    environment {
        version = "1.0"
        some_name = "Dev"
    }
    
    stages{
        stage ('build'){
            when{
                allOf {
                    environment name: "version", value: "1.0"
                    environment name: "some_name", value: "Dev"
                }
            }
            
            steps{
                echo "building  ${version}"
            }
        }
    }
}
